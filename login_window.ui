}

/* Push Buttons (Login, Register) */
QPushButton {
    background-color: yellow; /* Yellow background */
    color: black; /* Black text */
    border: 1px solid yellow; /* Yellow border */
    border-radius: 5px; /* Rounded corners */
    padding: 10px; /* Padding inside the button */
    font-size: 14px; /* Font size */
    font-weight: bold; /* Bold text */
}

/* Push Button Hover Effect */
QPushButton:hover {
    background-color: #cccc00; /* Darker yellow on hover */
    border: 1px solid #cccc00; /* Darker yellow border on hover */
}

/* Push Button Pressed Effect */
